<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="DesktopUserInterface.MainScene.BodyScene.EncryptDecrypt.EncryptAndDecrypt">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="NEVER" minWidth="30.0" prefWidth="30.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
        <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="NEVER" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="NEVER" />
    </rowConstraints>
    <children>
        <TextField fx:id="encryptDecryptTextBox" prefHeight="31.0" prefWidth="275.0" promptText="Put the word you want To encrypt/decrypt" GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.rowIndex="2" />
        <Button fx:id="encryptDecryptButton" mnemonicParsing="false" text="encrypt/decrypt" GridPane.columnIndex="2" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets right="10.0" />
            </GridPane.margin>
        </Button>
        <Button fx:id="ResetMachineStateButton" mnemonicParsing="false" text="Reset Machine State" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="3" />
        <Label fx:id="encryptDecryptTextLabel" text="encrypted/decrypted word" textFill="#1c11f2" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="4" />
        <ScrollPane fx:id="encryptDecryptWordsScrollPane" GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.rowIndex="4" GridPane.valignment="TOP">
            <content>
                <TextField fx:id="encryptedDecryptedWordText" editable="false" />
            </content>
        </ScrollPane>
    </children>
</GridPane>
