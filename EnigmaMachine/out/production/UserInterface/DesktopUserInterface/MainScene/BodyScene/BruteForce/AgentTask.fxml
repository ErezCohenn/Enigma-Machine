            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="taskTotalTimeLabel" text="Task total time:" GridPane.rowIndex="1">
