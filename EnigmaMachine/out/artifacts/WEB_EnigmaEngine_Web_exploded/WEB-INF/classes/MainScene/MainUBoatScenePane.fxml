<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="100.0" minWidth="100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainScene.MainUBoatScenePaneController">
   <content>
      <BorderPane minHeight="700.0" minWidth="1200.0" prefWidth="1280.0">
         <top>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
                  <fx:include fx:id="topBorderPane" source="../TopBorderPane/TopBorderPane.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </top>
         <center>
            <TabPane minHeight="300.0" minWidth="300.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Machine">
                     <content>
                      <fx:include fx:id="machineGrid" minHeight="500.0" minWidth="500.0" source="../MainScene/UBoatMachinePane/UBoatMachinePane.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="20.0" />
                     </content>
                </Tab>
                <Tab text="Contest">
                     <content>
                        <fx:include fx:id="UBoatCompetitionPane" source="../MainScene/CompetitionPane/UBoatCompetitionPane.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="20.0" />
                     </content>
                </Tab>
              </tabs>
            </TabPane>
         </center>
         <bottom>
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#onLogoutButtonClicked" prefHeight="34.0" prefWidth="177.0" text="Logout" textFill="#e80b0b" BorderPane.alignment="CENTER_RIGHT">
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </BorderPane.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </bottom>
      </BorderPane>
   </content>
</ScrollPane>
